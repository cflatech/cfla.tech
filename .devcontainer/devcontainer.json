{
  "name": "dev",
  "dockerComposeFile": ["compose.yml"],
  "service": "dev",
  "remoteUser": "devuser",
  "workspaceFolder": "/workspace",
  "remoteEnv": {
    "EDITOR": "code --wait"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "exiasr.hadolint",
        "ms-azuretools.vscode-docker",
        "vivaxy.vscode-conventional-commits",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}
